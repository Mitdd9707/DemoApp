{
  "name": "DemoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:saas": "ENVFILE=.env react-native run-android",
    "android:whiteLabel": "ENVFILE=.env.whiteLabel react-native run-android",
    "ios:saas": "ENVFILE=.env xcodebuild -scheme MyApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "ios:whitelabel": "ENVFILE=.env.whiteLabel xcodebuild -scheme MyApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.1",
    "@react-navigation/stack": "^6.4.1",
    "final-form": "^4.20.10",
    "formik": "^2.4.6",
    "react": "18.2.0",
    "react-final-form": "^6.5.9",
    "react-native": "0.74.3",
    "react-native-config": "^1.5.2",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-navigation": "^5.0.0",
    "styled-components": "^6.1.12",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
